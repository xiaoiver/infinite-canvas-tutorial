    "@lit/task": "^1.0.1",
    "@loaders.gl/core": "^4.2.2",
    "@loaders.gl/images": "^4.2.2",
    "@spectrum-web-components/icon": "^1.8.0",
    "@spectrum-web-components/icons-workflow": "^1.8.0",
